{
    "builds": [
        {
            "src": "./search_app.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./search_app.py"
        }
    ]
}