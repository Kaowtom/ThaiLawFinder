{
    "version": 2,
    "builds": [
        {
            "src": "./search_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "search_app.py"
        }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    }
}